{
  "name": "road-runner-studio",
  "private": true,
  "version": "1.0.0",
  "description": "Premium AI image and SVG generation studio",
  "author": "FDTiger777",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "wait-on http://localhost:3000 && electron .",
    "electron:build": "vite build && electron-builder"
  },
  "build": {
    "appId": "com.roadrunner.studio",
    "productName": "Road Runner Studio",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "public/icon.ico"
    }
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.9",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-is-dev": "^3.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^9.0.3"
  }
}